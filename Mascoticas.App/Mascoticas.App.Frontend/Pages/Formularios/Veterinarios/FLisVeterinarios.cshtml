@page
@model Mascoticas.App.Frontend.Pages.FLisVeterinariosModel
@{
}
<h2>
    LISTADO DE VETERINARIOS
</h2>
<table class= "table">

    <thead>
            <tr>
                <th>Tarje Profesional</th>
                <th>Nombres </th>
                <th>Apellidos</th>
                <th>Detalle</th>
                <th>Editar</th>
                
            </tr>
    </thead>

                @foreach (var Veterinario in Model.Veterinarios)
                {
                    <tr>
                        <td>@Veterinario.TarjetaProfesional</td>
                        <td>@Veterinario.Nombres</td>
                        <td>@Veterinario.Apellidos</td>
                        
                        
                    <td>
                <a class="btn btn-primary table-btn" asp-page="./FInscripcionVeterinario" asp-route-VeterinarioId="@Veterinario.Id">
                    <i class="fas fa-info-circle fa-lg"></i </a>
                </td>

                <td>
                <a class="btn btn-primary table-btn" asp-page="./FInscripcionVeterinario" asp-route-VeterinarioId="@Veterinario.Id">
                    <i class="fas fa-info-circle fa-lg"></i </a>
                </td>

                    </tr>
                
                    
                } 

            </table>