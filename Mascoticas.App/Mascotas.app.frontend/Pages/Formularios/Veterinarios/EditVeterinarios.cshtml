@page
@model Mascotas.app.frontend.Pages.EditVeterinariosModel
@{
}
<h2>Edición de Veterinarios :@Model.Veterinario.Nombre</h2>
<form method="post">
     <input type="hidden" asp-for="Veterinario.TarjetaProfesional"/>
    <div class="form-group">
        <label asp-for="Veterinario.Nombres"></label>
        <input asp-for="Veterinario.Nombres" class="form-control">
        <span class="text-danger" asp-validation-for="Veterinario.Nombres"></span>
    </div>
    <div class="form-group">
        <label asp-for="Veterinario.Apellidos"></label>
        <input asp-for="Veterinario.Apellidos" class="form-control">
        <span class="text-danger" asp-validation-for="Veterinario.Apellidos"></span>
    </div>

    <div class="form-group">
        <label asp-for="Veterinario.Celular"></label>
        <input asp-for="Veterinario.Celular" class="form-control">
        <span class="text-danger" asp-validation-for="Veterinario.Celular"></span>
    </div>

<div class="form-group">
        <label asp-for="Veterinario.Email"></label>
        <input asp-for="Veterinario.Email" class="form-control">
        <span class="text-danger" asp-validation-for="Veterinario.Email"></span>
    </div>

    <button type="submit" class="btn btn-primary">Grabar</button>
</form>
<a asp-page="./FlisVeterinarios" class="btn-default">Lista de Veterinarios </a>